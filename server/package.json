{
  "name": "server",
  "version": "1.0.0",
  "description": "Express server",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "nodemon",
    "build": "tsc",
    "watch-ts": "tsc -w",
    "run:dev": "yarn build && yarn start",
    "db:migrate:remote": "cd src/supabase &&supabase db push",
    "db:reset": "cd src/supabase && supabase db reset",
    "db:new": "cd src/supabase && supabase migration new",
    "db:start:local": "cd src/supabase && supabase start",
    "db:stop:local": "cd src/supabase && supabase stop",
    "db:status": "cd src/supabase && supabase status"
  },
  "dependencies": {
    "@googleapis/docs": "^3.3.0",
    "@googleapis/drive": "^8.16.0",
    "@supabase/supabase-js": "^2.49.3",
    "@types/html-to-text": "^9.0.4",
    "@types/lodash": "^4.17.16",
    "cors": "^2.8.5",
    "dedent": "^1.5.3",
    "dotenv": "^16.4.7",
    "express": "^4.18.2",
    "html-to-text": "^9.0.5",
    "jsdom": "^26.0.0",
    "lodash": "^4.17.21",
    "openai": "^4.86.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.1",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^20.10.5",
    "nodemon": "^3.0.2",
    "supabase": "^2.20.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
